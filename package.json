{
  "name": "fe-wallet",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tomidamianovich.github.io/fe-wallet/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "axios": "^0.27.2",
    "gh-pages": "^4.0.0",
    "i18next": "^21.8.8",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "install": "^0.13.0",
    "json-server": "^0.17.0",
    "npm": "^8.12.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.17.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "react-scripts": "5.0.1",
    "sass": "^1.52.1",
    "tsc-files": "^1.1.3"
  },
  "scripts": {
    "start": "npm run start:prod",
    "start-server": "json-server --watch db.json --port 3001",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "env-cmd -f ./.dev.env react-scripts start",
    "build:dev": "env-cmd -f ./.dev.env npm run-script build",
    "start:prod": "env-cmd -f ./.prod.env react-scripts start",
    "build:prod": "env-cmd -f ./.prod.env npm run-script build",
    "lint": "eslint src --color",
    "prettier": "prettier src -check",
    "husky-install": "husky install",
    "lint-staged": "lint-staged --config lint-staged.js",
    "typescript": "tsc --project tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
